[binaries]
c = '/usr/bin/clang-11'
cpp = '/usr/bin/clang++-11'
ar = '/usr/bin/llvm-ar-11'
strip = '/usr/bin/llvm-strip-11'
objcopy = '/usr/bin/llvm-objcopy-11'
ld = '/usr/bin/llvm-ld-11'
exe_wrapper = ['sde64', '-future', '--']

[properties]
c_args = ['-march=bdver4', '-Weverything', '-Wno-newline-eof', '-Wno-missing-variable-declarations', '-Werror']
cpp_args = ['-march=bdver4', '-Weverything', '-Wno-newline-eof', '-Wno-missing-variable-declarations', '-Werror']

[host_machine]
system = 'linux'
cpu_family = 'x86_64-all'
cpu = 'x86_64'
endian = 'little'
